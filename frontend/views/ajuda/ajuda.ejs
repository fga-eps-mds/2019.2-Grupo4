  <script src="/ajuda/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/ajuda/lib/easing/easing.min.js"></script>
  <script src="/ajuda/lib/mobile-nav/mobile-nav.js"></script>
  <script src="/ajuda/lib/wow/wow.min.js"></script>
  <script src="/ajuda/lib/waypoints/waypoints.min.js"></script>
  <script src="/ajuda/lib/counterup/counterup.min.js"></script>
  <script src="/ajuda/lib/owlcarousel/owl.carousel.min.js"></script>
  <script src="/ajuda/lib/isotope/isotope.pkgd.min.js"></script>
  <script src="/ajuda/lib/lightbox/js/lightbox.min.js"></script>
  <script src="/ajuda/js/main.js"></script>

</body>

</html>
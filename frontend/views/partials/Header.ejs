<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/header/style/header.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href='https://fonts.googleapis.com/css?family=Knewave' rel='stylesheet'>
</head>
<header>
    <div class="topnav" id="myTopnav">
        <a href="/forms" id="logo">QM</a>
        <a href="#" onclick="abrir('http://localhost:3000/ajuda')">Ajuda</a>
        <a href="/users/logout">Logout</a>
        <a href="javascript:void(1);" class="icon" onclick="myFunction()">
            <i class="fa fa-bars"></i>
        </a>
    </div>
    <script>
        myFunction = () => {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }

        abrir = (URL) => {
            window.open(URL, 'janela',
                'width=795, height=590, top=100, left=699,scrollbars=no, status=no, toolbar=no, location=center, menubar=no, resizable=no, fullscreen=no')
        }
    </script>
</header>